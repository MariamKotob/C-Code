@{
                /**/

                Layout = "~/Views/Shared/_Layout.cshtml";
}

<style>
    /* Always set the map height explicitly to define the size of the div
       * element that contains the map. */
    #map {
        height: 400px;
        width:800px;
    }
    
</style>
<br />
<div id="class">
    <div id="map"></div>
    <script>
        var map;
        function initMap() {
            map = new google.maps.Map(document.getElementById('map'), {
                center: { lat: 29.966833, lng: 32.549809 },
                zoom: 11
            });
            var marker = new google.maps.Marker({ position: { lat: 29.966833, lng: 32.549809 }, map: map });
        }
    </script>
    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyCJtlYbFgHcC_j-iu88pNOQIR_2mTx7av8&callback=initMap"
            async defer></script>
</div>